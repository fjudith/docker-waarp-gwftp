<config>
  <identity>
    <hostid>fw-client</hostid>
    <sslhostid>fw-client-ssl</sslhostid>
    <cryptokey>/etc/waarp/certs/cryptokey.des</cryptokey>
    <authentfile></authentfile>
  </identity>
  <client></client>
  <ssl>
    <keypath>/etc/waarp/certs/key.jks</keypath>
    <keystorepass>password</keystorepass>
    <keypass>password</keypass>
    <trustkeypath>/etc/waarp/certs/trustkey.jks</trustkeypath>
    <trustkeystorepass>password</trustkeystorepass>
    <trustuseclientauthenticate>True</trustuseclientauthenticate>
  </ssl>
  <directory>
    <serverhome>/var/lib/waarp/fw-client</serverhome>
    <in>in</in>
    <out>out</out>
    <arch>arch</arch>
    <work>work</work>
    <conf>conf</conf>
  </directory>
  <limit>
    <serverthread>8</serverthread>
    <clientthread>80</clientthread>
    <memorylimit>4000000000</memorylimit>
    <runlimit>10000</runlimit>
    <delaycommand>5000</delaycommand>
    <delayretry>10000</delayretry>
    <timeoutcon>10000</timeoutcon>
  </limit>
  <db>
    <dbdriver>h2</dbdriver>
    <dbserver>jdbc:h2:/var/lib/waarp/server1/db/waarp;MODE=ORACLE;AUTO_SERVER=TRUE</dbserver>
    <dbuser>waarp</dbuser>
    <dbpasswd>waarp</dbpasswd>
  </db>
    <spooleddaemon>
        <stopfile>/var/lib/waarp/fw-client/spool/stop</stopfile>
        <logWarn>False</logWarn>
        <spooled>
            <info></info>
            <name>fw-client</name>
            <to>server1</to>
            <rule>fileWatcher</rule>
            <statusfile>/var/lib/waarp/fw-client/status.json</statusfile>
            <directory>/var/lib/waarp/fw-client/spool/</directory>
            <regex>.*</regex>
            <recursive>True</recursive>
            <elapse>5000</elapse>
            <submit>False</submit>
            <parallel>True</parallel>
            <limitParallel>1</limitParallel>
            <waarp>server1</waarp>
            <elapseWaarp>5000</elapseWaarp>
        </spooled>
    </spooleddaemon>
</config>
